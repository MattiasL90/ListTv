
@{
    ViewBag.Title = "AddChannel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<string>

               <div>
                   <form method="get" action="@Url.Action("AddFavorite", "VM")">
                       <select>
                           @foreach (var item in Model)
        {
                           <option value="@item">@item</option>
        }
                       </select>
                       <button type="submit" name="date" id="date" value="@DateTime.Now.Date">Lägg till</button>
                       </form>

               </div>
